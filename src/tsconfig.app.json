{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "jszip": [
        "./node_modules/jszip/dist/jszip.min.js"
      ]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
